{
  "name": "crypto-analytics-backend",
  "version": "1.0.0",
  "description": "GraphQL backend for crypto analytics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate:pg",
